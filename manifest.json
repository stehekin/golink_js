{
  "manifest_version": 3,
  "name": "golink",
  "version": "1.0.0",
  "description": "A Chrome extension for managing and accessing go links",
  "permissions": [
    "storage",
    "activeTab",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "golink"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "web_accessible_resources": [
    {
      "resources": ["404.html"],
      "matches": ["<all_urls>"]
    }
  ]
}